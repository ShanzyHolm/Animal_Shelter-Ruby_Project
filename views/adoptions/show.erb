<h1>All Adoptions</h1>

<% @adoptions.each do |adoption| %>
  <p> Owner Name: <%= adoption.owner.first_name %> <%= adoption.owner.last_name %> </p>
  <p> Animal Name: <%= adoption.animal.name %> </p>



  <p> <a href="/adoptions/<%= adoption.id() %> ">Show Adoption Details</a> </p>
  <hr />
<% end %>
